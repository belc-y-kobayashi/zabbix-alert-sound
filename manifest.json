{
  "manifest_version": 3,
  "name": "Zabbix Custom Dashboard",
  "version": "1.2.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["document_start.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["inject.js", "error.wav"],
      "matches": ["<all_urls>"]
    }
  ]
}
